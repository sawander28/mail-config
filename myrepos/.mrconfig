# vim: fenc=utf-8 ft=dosini ts=2 sts=2 sw=2 et


[$HOME/myrepos/void-packages]
checkout = git clone https://github.com/void-linux/void-packages.git
update = git fetch upstream master

[void/hrmpf]
checkout = git clone 'git@github.com:leahneukirchen/hrmpf.git' 'hrmpf'

[void/void-containers]
checkout = git clone 'https://github.com/void-linux/void-containers.git' 'void-containers'

[void/void-mklive]
checkout = git clone 'https://github.com/void-linux/void-mklive.git' 'void-mklive'

[void/xbps]
checkout = git clone 'https://github.com/void-linux/xbps.git' 'xbps'

[void/ungoogled-chromium-void]
checkout = git clone 'https://github.com/DAINRA/ungoogled-chromium-void.git' 'ungoogled-chromium-void'

[$HOME/myrepos/openbsd/src]
checkout = cvs -d anoncvs@ftp.hostserver.de:/cvs co -d src src
update = cvs update
